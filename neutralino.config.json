{
    "$schema"         : "./api/json/models/neutralino.config.schema.json",
    "applicationId"   : "js.neutralino.sample",
    "defaultMode"     : "window",
    "port"            : 0,
    "url"             : "/test/resources/",
    "enableHTTPServer": true,
    "enableNativeAPI" : true,
    "nativeBlockList" : [],
    "globalVariables" : {
        "API_DEFS": "Test Value"
    },
    "modes": {
        "window": {
            "title"             : "Neutralinojs Demo API",
            "width"             : 800,
            "height"            : 500,
            "minWidth"          : 400,
            "minHeight"         : 200,
            "fullScreen"        : false,
            "alwaysOnTop"       : false,
            "icon"              : "/test/resources/icons/appIcon.png",
            "enableInspector"   : true,
            "borderless"        : false,
            "maximize"          : false,
            "hidden"            : false,
            "resizable"         : true,
            "exitProcessOnClose": false
        },
        "browser": {},
        "cloud": {}
    },
    "cli": {
        "binaryName": "neuapi",
        "resourcesPath": "/test/resources/",
        "clientLibrary": "/test/resources/js/neutralino.js"
    }
}
